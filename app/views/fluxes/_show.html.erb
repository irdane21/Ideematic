
<div class="col-6">
  <div class="card">
    <div class="title">
      <h2><%= flux.Title %></h2>
    </div>
    <ul class="card-overflow" id="flux<%= flux.id %>">
      <% @articles.each do |article| %>
        <%= render 'articles/show', article: article %>
      <% end %>
    </ul>
    <%= will_paginate @articles %>
  </div>
</div>
