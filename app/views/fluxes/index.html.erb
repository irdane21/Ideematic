<div class="wrapper">
  <div class="header">
    <div id="addform" class="hide">
      <%= render 'fluxes/form', flux: @flux %>
    </div>
    <button id="addflux">Ajouter</button>
  </div>
  <div class="container">
    <div class="row margin-top" id="fluxes">
      <% @fluxes.each do |flux| %>
        <%= render 'fluxes/show', flux: flux %>
      <% end %>
    </div>
  </div>
</div>
